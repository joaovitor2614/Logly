{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/app/(.*)",
      "dest": "/web/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/dist/index.html"
    }
  ]
}